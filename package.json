{
  "name": "yarn-workspace",
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "check": "yarn workspaces list --json",
    "build:all": "yarn workspaces foreach -A run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "latest",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "pinst": "^3.0.0",
    "prettier": "latest",
    "typescript": "latest"
  },
  "workspaces": [
    "packages/*",
    "hook"
  ],
  "dependencies": {
    "esbuild": "^0.19.8",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "yarn": "4.x"
  }
}

{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "module": "commonjs",
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": false,
    "incremental": true,
    "types": ["node", "jest", "@testing-library/jest-dom"],
    "lib": ["es5", "es6", "dom", "ESNext", "dom.iterable"]
  }
}
